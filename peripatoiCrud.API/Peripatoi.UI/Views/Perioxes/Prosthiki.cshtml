@model Peripatoi.UI.Models.ProsthikiPerioxhsViewModel

@{
}

<h1 class="mt-3">Προσθήκη Περιοχής</h1>

<form method="post">
    <div class="mt-3">
        <label class="form-label">Κωδικός</label>
        <input type="text" class="form-control" asp-for="Kwdikos"/>
    </div>

    <div class="mt-3">
        <label class="form-label">Όνομα</label>
        <input type="text" class="form-control" asp-for="Onoma" />
    </div>

    <div class="mt-3">
        <label class="form-label">Σύνδεσμος Εικόνας</label>
        <input type="text" class="form-control" asp-for="EikonaUrl" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Αποθήκευση Περιοχής</button>
    </div>

</form>
